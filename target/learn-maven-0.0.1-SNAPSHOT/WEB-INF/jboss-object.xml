<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE deployments PUBLIC "-//JBoss Portal//DTD Portal Object 2.6//EN" "http://www.jboss.org/portal/dtd/portal-object_2_6.dtd">
<deployments>
	<deployment>
		<parent-ref />
		<if-exists>overwrite</if-exists>
		<portal>
			<portal-name>HelloWorldPortal</portal-name>
			<supported-modes>
				<mode>view</mode>
			</supported-modes>
			<supported-window-states>
				<window-state>normal</window-state>
			</supported-window-states>
			<page>
				<page-name>default</page-name>
				<window>
					<window-name>HelloWorldWindow</window-name>
					<instance-ref>HelloWorldInstance</instance-ref>
					<region>center</region>
					<height>0</height>
				</window>
			</page>
		</portal>
	</deployment>
</deployments>